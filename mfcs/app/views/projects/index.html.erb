<div class="container-fluid">
  <h1> List Objects - Select Project</h1> 

  <table class="table table-striped">
    <thead>
      <tr>
        <th> Project Name </th>
        <th> Project ID (Short Name) </th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td> <%= project.name %> </td>
          <td> <%= project.project_id %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>  
</div>
		